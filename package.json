{
  "name": "react-bolierplate-es6",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "dev": "webpack serve --config ./webpack.config.js --mode development",
    "build": "rm -rf dist && webpack --mode production",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaimedp/react-bolierplate-es6.git"
  },
  "author": "Jaime del Palacio <jaimedp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaimedp/react-bolierplate-es6/issues"
  },
  "homepage": "https://github.com/jaimedp/react-bolierplate-es6#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "sourceMap": false,
    "instrument": false,
    "extension": [
      ".jsx",
      ".js"
    ],
    "exclude": [
      "**/*.spec.js",
      "test/**/*.js",
      "public",
      "dist"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
