    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "precss": "^4.0.0",
    "react-hot-loader": "^4.13.0",
    "sinon": "^9.2.2",
    "sinon-chai": "^3.5.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^5.2.2"
