{
  "name": "react-bolierplate-es6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "rm -rf public && webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --content-base src/",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaimedp/react-bolierplate-es6.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaimedp/react-bolierplate-es6/issues"
  },
  "homepage": "https://github.com/jaimedp/react-bolierplate-es6#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "sourceMap": false,
    "instrument": false,
    "extension": [
