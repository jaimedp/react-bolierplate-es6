    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^17.0.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.0.0",
