    "precss": "^2.0.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.12.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
